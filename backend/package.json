{
  "name": "nestjs-vertical-slice-template-backend",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "keywords": [
    "typescript",
    "nestjs"
  ],
  "engines": {
    "node": ">=22.x",
    "pnpm": ">=10.x"
  },
  "scripts": {
    "husky": "husky",
    "prebuild": "rimraf dist",
    "build:prod": "nest build --path tsconfig.prod.json --webpack",
    "build": "nest build --path tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:nodemon": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register src/main.ts",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts",
    "lint:fix": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts --fix",
    "lint:file": "eslint --ignore-path .gitignore",
    "lint:yaml": "chmod +x scripts/lint_yaml.sh && ./scripts/lint_yaml.sh",
    "test": "jest --config jest.config.ts",
    "test:unit": "jest --config jest.config.ts \"test/unit-tests/\"",
    "test:integration": "npm run build && jest --config jest.config.ts \"test/integration-tests/\"",
    "test:e2e": "npm run build && jest --config jest.config.ts \"test/e2e-tests/\"",
    "test:performance": "jest --config jest.config.ts \"test/performance/\"",
    "test:cov": "jest --config jest.config.ts --coverage --coverageDirectory ./coverage",
    "test:watch": "jest --config jest.config.ts --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --config jest.config.ts --runInBand",
    "build:clean": "rimraf dist; exit 0",
    "typeorm": "npm run build && npx typeorm-ts-node-esm -d dist/src/database/data-source-local.config.js",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:show": "npm run typeorm migration:show",
    "migration:create": "typeorm migration:create",
    "schema:sync": "npm run typeorm schema:sync",
    "drop:database": "npm run typeorm schema:drop",
    "typeorm-extension": "npm run build && ts-node ./node_modules/typeorm-extension/bin/cli.cjs -d ./dist/src/database/data-source-local.config.js",
    "seed:run": "npm run typeorm-extension seed:run",
    "seed:create": "npm run typeorm-extension seed:create",
    "db:create": "npm run typeorm-extension db:create",
    "db:drop": "npm run typeorm-extension db:drop"
  },
